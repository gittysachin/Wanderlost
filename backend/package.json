{
  "name": "ecommerce-prisma-graphql-apollo-server",
  "private": true,
  "engines": {
    "node": "13.x"
  },
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --no-notify --respawn --transpileOnly -r dotenv/config src/index",
    "start": "node dist/index",
    "clean": "rm -rf dist",
    "build": "tsc",
    "build:clean": "npm -s run clean && npm -s run generate && tsc",
    "studio": "prisma studio --experimental",
    "migrate:save": "npx prisma migrate save --experimental",
    "migrate:up": "npx prisma migrate up --experimental",
    "generate": "npx prisma generate"
  },
  "dependencies": {
    "@prisma/client": "^2.0.0-beta.3",
    "apollo-server-express": "^2.14.2",
    "bcryptjs": "^2.4.3",
    "concurrently": "^5.1.0",
    "cookie-parser": "^1.4.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "graphql": "^14.2.1",
    "jsonwebtoken": "^8.5.1",
    "nodemailer": "^6.4.2",
    "stripe": "^8.12.0"
  },
  "devDependencies": {
    "@prisma/cli": "^2.0.0-beta.3",
    "@types/bcryptjs": "^2.4.2",
    "@types/cookie-parser": "^1.4.2",
    "@types/jsonwebtoken": "^8.3.9",
    "@types/node": "^13.13.1",
    "@types/nodemailer": "^6.4.0",
    "ts-node": "^8.9.0",
    "ts-node-dev": "^1.0.0-pre.44",
    "typescript": "^3.8.3"
  },
  "prettier": {
    "singleQuote": true,
    "semi": false,
    "trailingComma": "all"
  }
}
